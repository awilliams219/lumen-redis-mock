{
    "name": "awilliams219/lumen-redis-mock",
    "description": "This Lumen package provides a Redis mock for your tests",
    "keywords": [
        "localization",
        "language",
        "laravel",
        "php"
    ],
    "homepage": "https://github.com/awilliams219/lumen-redis-mock",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Williams",
            "email": "adam.williams@nationwidegroup.org"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/lumen-framework": "^5.8.30|^6.0|^7.0|^8.0|^9.0",
        "predis/predis": "^1.1",
        "m6web/redis-mock": "^4.2|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.5.10",
        "orchestra/testbench": "~3.8.0|^4.0|^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lunaweb\\RedisMock\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lunaweb\\RedisMock\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "vendor/phpunit/phpunit/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lunaweb\\RedisMock\\Providers\\RedisMockServiceProvider"
            ]
        }
    }
}
